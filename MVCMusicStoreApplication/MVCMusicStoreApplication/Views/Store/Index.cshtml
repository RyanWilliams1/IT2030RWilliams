@model IEnumerable<MVCMusicStoreApplication.Models.Album>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        string titleName = item.Title;
        <tr>
            <td>
                @Html.ActionLink(titleName, "Details", new { id = item.AlbumId })
            </td>
        </tr>
    }

</table>

